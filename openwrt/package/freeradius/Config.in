config BR2_PACKAGE_FREERADIUS
	prompt "freeradius - a flexible RADIUS server"
	tristate 
	default m if CONFIG_DEVEL
	help
	  A flexible RADIUS server
	  
	  http://www.freeradius.org/
	  
	  Depends: libltdl, libopenssl, libpthread
	  

