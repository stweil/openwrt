config BR2_PACKAGE_LIGHTTPD
	prompt "lighttpd - a flexible and lightweight web server"
	tristate 
	default m if CONFIG_DEVEL
	help
	  A flexible and lightweight web server.
	  
	  http://www.lighttpd.net/
	  
	  Depends: libopenssl, libpcre
	  

