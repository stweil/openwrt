config BR2_PACKAGE_KMOD_IPTABLES_V4
	tristate "Basic set of kernel modules for iptables"
	default y
	help
	  Kernel modules for IPv4 firewalling

config BR2_PACKAGE_KMOD_IPTABLES_V4_EXTRA
	tristate "Extra modules for iptables"
	default m
	help
